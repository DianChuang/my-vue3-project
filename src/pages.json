	"easycom": {
		"autoscan": true,
		"custom": {
			"^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue",
			// 以 Et 开头的组件，在 public-components 文件夹中查找引入（需要重启服务器）
			"^Et(.*)": "@/components/Et$1.vue"
		}
	},
